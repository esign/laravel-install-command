{
    "name": "esign/laravel-install-command",
    "description": "Simplify stub publishing and effortlessly manage Composer and Node packages",
    "keywords": [
        "esign",
        "install-command"
    ],
    "homepage": "https://github.com/esign/laravel-install-command",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Jordy Van der Haegen",
            "email": "jordy@esign.eu",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.5",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Esign\\InstallCommand\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Esign\\InstallCommand\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "phpcs": "vendor/bin/php-cs-fixer fix"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Esign\\InstallCommand\\InstallCommandServiceProvider"
            ],
            "aliases": {
                "InstallCommand": "Esign\\InstallCommand\\Facades\\InstallCommandFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
